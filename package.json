{
  "name": "navcertitrans",
  "version": "1.0.1",
  "description": "Navigateur personnalis√© pour CertiTrans",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.certitrans.browser",
    "productName": "NavCertiTrans",
    "files": [
      "src/**/*",
      "main.js",
      "preload.js"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
    "provider": "github",
    "owner": "Azed-Oume",
    "repo": "NavCertiTrans"
  },
  "win": {
    "target": "nsis",
    "icon": "assets/icon.ico"
  },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azed-Oume/NavCertiTrans.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Azed-Oume/NavCertiTrans/issues"
  },
  "homepage": "https://github.com/Azed-Oume/NavCertiTrans#readme",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "electron": "^35.1.5",
    "electron-builder": "^26.0.12"
  }
}
